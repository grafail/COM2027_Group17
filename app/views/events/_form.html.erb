<%= form_with(model: event, local: true) do |form| %>
  <% if event.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(event.errors.count, "error") %> prohibited this event from being saved:</h2>

      <ul>
      <% event.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :SellerID %>
    <%= form.text_field :SellerID %>
  </div>

  <div class="field">
    <%= form.label :Name %>
    <%= form.text_field :Name %>
  </div>

  <div class="field">
    <%= form.label :Address %>
    <%= form.text_field :Address %>
  </div>

  <div class="field">
    <%= form.label :Twitter %>
    <%= form.text_field :Twitter %>
  </div>

  <div class="field">
    <%= form.label :IsBusiness %>
    <%= form.check_box :IsBusiness %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
