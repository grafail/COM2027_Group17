<h1>Hi</h1>

<table>
  <thead>
  <tr>
    <th></th>
    <th>Event Name:</th>
    <th>Event Details:</th>
    <th>Tickets:</th>
    <th>Name of Ticket:</th>
    <th>Price:</th>
    <th>Quantity:</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @myEvents.each do |event| %>
  <%@ticket = Ticket.find_by(event:event.id) %>
    <tr>
      <td>
        <table>
          <tr>
            <td>
              <%= link_to 'Edit', edit_event_path(event) %>
            </td>
          </tr>
          <tr>
            <td>
              <%= link_to 'New Ticket', new_ticket_path(event_id: @event) %>
            </td>
          </tr>
        </table>
      </td>
      <td>
        <%= event.name %></td>
      <td>
        <table>
          <tr>
            <td>
              <%="Date & Time: #{event.eventDate}" %>
            </td>
          </tr>
          <tr>
            <td>
              <%="Location: #{event.location}" %>
            </td>
          </tr>
          <tr>
            <td>
              <%="Event Type: #{event.type}" %>
            </td>
          </tr>
          <tr>
            <td>
              <%="Description: #{event.description}" %>
            </td>
          </tr>
        </table>
      </td>

      <td><!--Tickets--></td>

      <td><%=@ticket.name %></td>
      <td><%="Â£#{@ticket.price}"%></td>
      <td><%=@ticket.quantity %></td>
    </tr>
  <% end %>
  </tbody>
</table>