<p id="notice"><%= notice %></p>

<h1>Events</h1>

<div class="container">
  <% @events.each do |event| %>
    <%= render partial: '/shared/eventItem', locals: { item: event } %>
  <% end %>
  <%= button_to 'New Event', new_event_path, :method => :get, :class => "btn btn-primary" %>
</div>

<br>


